# Global parameters
[global]
	workgroup = PROGRESSOFT
	realm = PROGRESSOFT.CLOUD
	netbios name = DC
	server role = active directory domain controller
	dns forwarder = 8.8.8.8
        ldap server require strong auth = no
[netlogon]
	path = /var/lib/samba/sysvol/progressoft.cloud/scripts
	read only = No

[sysvol]
	path = /var/lib/samba/sysvol
	read only = No
